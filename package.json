{
  "name": "ct-grunt-styleguide",
  "description": "Chrometoaster's KSS Styleguide Generator",
  "version": "1.1.0",
  "author": "Dan Smith (dan.smith@chrometoaster.com)",
  "homepage": "http://github.com/chrometoasters/ct-grunt-styleguide",
  "bugs": "http://github.com/chrometoasters/ct-grunt-styleguide/issues",
  "repository": {
    "type": "git",
    "url": "http://github.com/chrometoasters/ct-grunt-styleguide.git"
  },
  "project": {
    "name":         "Site Name",
    "banner":       "",
    "styleguide":   {
      "template":     "bower_components/ct-grunt-styleguide/template",
      "src":          "path/to/theme/css",
      "dest":         "path/to/theme/styleguide",
      "page":         "path/to/theme/styleguide-page.php",
      "width":        "922px",
      "designs":      ""
    }
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-kss": "~0.2.1",
    "grunt-string-replace": "~0.2.7",
    "load-grunt-tasks": "~0.4.0",
    "glob": "~3.2.1"
  }
}
